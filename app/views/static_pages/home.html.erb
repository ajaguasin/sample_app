<% if logged_in? %>
  <div class="row">
    <aside class="col-md-4">
      <section class="user_info">
        <%= render 'shared/user_info' %>
      </section>
      <section class="workoutpost_form">
        <%= render 'shared/workoutpost_form' %>
      </section>
    </aside>
    <div class="col-md-8">
      <h3>Workout Feed</h3>
      <%= render 'shared/feed' %>
    </div>
  </div>
<% else %>

<div class="center jumbotron">
  <h1>Welcome to the Exercise Guideline</h1>

  <h2>
    This is the home page for the Exercise Guideline application
  </h2>

  <%= link_to "Sign up now!", signup_path, class: "btn btn-lg btn-primary" %>
</div>

<% end %>
