<%= form_for(@workoutpost) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.text_area :content, placeholder: "Compose new workout log ..." %>
    <%= f.select :mood, options_for_mood , label: "Mood", class: "form-control" %>
    <%= f.select :length, options_for_length , label: "Length", class: "form-control" %>

  </div>
  <%= f.submit "Post", class: "btn btn-primary" %>
<% end %>